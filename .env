# API Configuration - Use nginx proxy instead of direct backend call
# VITE_API_BASE_URL=http://localhost:8080/api/v1  # Direct backend (causes CORS)
VITE_API_BASE_URL=/api/v1  # Nginx proxy (recommended)

# WebSocket Configuration  
VITE_WS_URL=ws://localhost:8080/ws

# OAuth Configuration (optional - có thể để trống cho development)
VITE_GOOGLE_CLIENT_ID=
VITE_FACEBOOK_CLIENT_ID=

# Environment
VITE_NODE_ENV=development

# PWA Configuration (optional)
VITE_VAPID_PUBLIC_KEY=

# Analytics (optional)
VITE_ANALYTICS_ID=
VITE_ENABLE_ANALYTICS=false