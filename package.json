{
  "name": "ai-content-automation-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:nginx": "BACKEND_PORT=8080 npm run build && ./scripts/start-nginx-dev.sh",
    "dev:nginx:prod": "BACKEND_PORT=8081 npm run build && ./scripts/start-nginx-dev.sh",
    "build": "tsc --project tsconfig.build.json && NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:skip-check": "vite build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:prod": "NODE_ENV=production npm run build:skip-check",
    "build:vercel": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --run",
    "test:unit": "vitest --run src/**/*.test.{ts,tsx}",
    "test:integration": "vitest --run src/test/integration/**/*.test.{ts,tsx}",
    "test:performance": "vitest --run src/test/performance/**/*.test.{ts,tsx}",
    "test:security": "vitest --run src/test/security/**/*.test.{ts,tsx}",
    "test:chaos": "vitest --run src/test/chaos/**/*.test.{ts,tsx}",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:ci": "cypress run --browser chrome --headless",
    "test:visual": "cypress run --spec 'cypress/e2e/visual/**/*.cy.ts'",
    "test:all": "npm run test:coverage && npm run test:e2e && npm run test:security",
    "test:ci": "npm run test:coverage && npm run test:e2e:ci && npm run test:security",
    "perf:lighthouse": "lhci autorun",
    "perf:bundle": "npm run build:analyze",
    "security:audit": "npm audit --audit-level moderate",
    "security:scan": "npm run test:security"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.17.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@mui/x-date-pickers": "^6.20.2",
    "@mui/x-date-pickers-pro": "^6.18.2",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "@tinymce/tinymce-react": "^6.3.0",
    "@types/diff": "^7.0.2",
    "@types/react-beautiful-dnd": "^13.1.8",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "diff": "^8.0.2",
    "emoji-picker-react": "^4.13.3",
    "framer-motion": "^10.18.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.544.0",
    "moment": "^2.29.4",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-big-calendar": "^1.8.5",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.1",
    "react-swipeable": "^7.0.1",
    "react-toastify": "^11.0.5",
    "reactflow": "^11.11.4",
    "recharts": "^3.2.1",
    "socket.io-client": "^4.7.5",
    "typescript": "^5.2.2",
    "yup": "^1.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.20",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.0",
    "@types/node": "^24.5.2",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.37",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "cypress": "^13.6.2",
    "cypress-visual-regression": "^5.0.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.0.1",
    "msw": "^2.0.11",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.0.4"
  }
}
